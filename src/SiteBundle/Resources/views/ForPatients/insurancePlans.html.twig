{% set title = 'Insurance Plans' %}
{% set meta_keywords = null %}
{% set meta_description = null %}
{% set meta_author = null %}
{% extends "::base.html.twig" %}

{% block title %}SiteBundle:ForPatients:insurancePlans{% endblock %}

{% block body %}
	<div class="page-header">
		<div class="container">
			<h1 class="title">{{ title }}</h1>
		</div>
	</div>
	<section id="insurance-plans" class="page-section">
		<div class="container text-center">
			<div class="row">
				<div class="col-sm-12">
					<p class="lead">{{ company_name }} is associated with the following insurance plans:</p>
				</div>
			</div>
			<div class="row">
				<div class="col-xs-6 col-sm-2 col-sm-offset-2">
					<div class="col-sm-12 light-bg border-color pad-10 bottom-margin-10">
						<img src="{{ asset('bundles/site/images/sections/insurance/blue-cross-blue-shield.png') }}" width="100%" alt="Blue Cross Blue Shield" data-toggle="tooltip" data-html="true" data-placement="bottom" title="<ul><li>HonorHealth Employee Plan</li></li>Medicare Advantage</li><li>Alliance</li></ul>" />
					</div>
				</div>
				<div class="col-xs-6 col-sm-2">
					<div class="col-sm-12 light-bg border-color pad-10 bottom-margin-10">
						<img src="{{ asset('bundles/site/images/sections/insurance/cigna.png') }}" width="100%" alt="Cigna" data-toggle="tooltip" data-html="true" data-placement="bottom" title="<ul><li>Open Access Plus</li><li>Local Plus</li></ul>" />
					</div>
				</div>
				<div class="col-xs-6 col-sm-2">
					<div class="col-sm-12 light-bg border-color pad-10 bottom-margin-10">
						<img src="{{ asset('bundles/site/images/sections/insurance/humana.png') }}" width="100%" alt="Humana" data-toggle="tooltip" data-html="true" data-placement="bottom" title="<ul><li>Humana Gold Plus</li></ul>" />
					</div>
				</div>
				<div class="col-xs-6 col-sm-2">
					<div class="col-sm-12 light-bg border-color pad-10 bottom-margin-10">
						<img src="{{ asset('bundles/site/images/sections/insurance/medicare.png') }}" width="100%" alt="Medicare" data-toggle="tooltip" data-html="true" data-placement="bottom" title="<ul><li>Medicare FFS</li></ul>" />
					</div>
				</div>
				
			</div>
		</div>
	</section>
	<!-- addition-info -->
	<div id="learn-more" class="light-bg call-to-action black text-center">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<a class="black" href="{{ path('our_physicians') }}">Find an SHP doctor</a> near you
				</div>
			</div>
		</div>
	</div>
{% endblock %}
